{
	"name": "receipt-cli",
	"repository": {
		"type": "git",
		"url": "https://github.com/Chef-UI/receipt-cli"
	},
	"version": "0.0.5",
	"description": "Helpers to create components, containers and pages into react projects.",
	"keywords": [
		"react",
		"receipt",
		"cli",
		"helper",
		"template",
		"component",
		"container",
		"pages",
		"boilerplate",
		"gluegun",
		"javascript"
	],
	"bin": {
		"rc": "bin/receipt-cli"
	},
	"scripts": {
		"lint": "prettier --write '**/*.{js,json,test.js}'",
		"test:all": "jest --config ./jest.config.json",
		"test:watch": "jest --watch --config ./jest.config.json",
		"test:component": "jest -- __tests__/new:component.test.js --config ./jest.config.json",
		"test:container": "jest -- __tests__/new:container.test.js --config ./jest.config.json",
		"test:page": "jest -- __tests__/new:page.test.js --config ./jest.config.json",
		"test:template": "jest -- __tests__/new:template.test.js --config ./jest.config.json",
		"watch": "jest --watch",
		"snapupdate": "jest --updateSnapshot",
		"coverage": "jest --coverage",
		"prepare": "husky install"
	},
	"files": [
		"src",
		"LICENSE",
		"readme.md",
		"docs",
		"bin"
	],
	"license": "MIT",
	"dependencies": {
		"gluegun": "latest"
	},
	"devDependencies": {
		"husky": "^7.0.4",
		"jest": "^24.1.0",
		"lint-staged": "^12.1.2",
		"prettier": "^2.5.0",
		"standard": "^12.0.1"
	}
}
